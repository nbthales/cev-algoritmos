algoritmo "conte10a0"
var
   contador: Inteiro
inicio
      contador <- 10
      Enquanto (contador >= 0) faca
               EscrevaL(contador)
               contador <- contador -1
             //contador <- contador -2     tira de 2 em 2
      FimEnquanto
      Escreval("Terminei de contar")
fimalgoritmo
